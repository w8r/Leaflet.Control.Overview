{
  "name": "leaflet-overviewmap",
  "version": "0.0.1",
  "description": "Overview map control",
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "lint": "eslint ./src/",
    "lint:watch": "nodemon --exec 'npm run lint'",
    "demo": "watchify -v -d demo/app.js -o demo/bundle.js",
    "start": "npm run demo & npm run server & npm run lint:watch & npm run test:watch",
    "server": "http-server -p 3003"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "keywords": [
    "leaflet",
    "control",
    "overview",
    "minimap"
  ],
  "author": "Alexander Milevski <info@w8r.name>",
  "license": "MIT",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "electron": "^1.4.4",
    "eslint": "^3.9.0",
    "http-server": "^0.9.0",
    "karma": "^1.3.0",
    "karma-browserify": "^5.1.0",
    "karma-electron": "^5.1.1",
    "karma-spec-reporter": "0.0.26",
    "karma-tap": "^3.1.1",
    "leaflet": "^1.0.1",
    "nodemon": "^1.11.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "leaflet-path-drag": "^1.0.1"
  }
}
